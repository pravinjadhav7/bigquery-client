{
  "name": "bigquery-client",
  "version": "1.0.3",
  "description": "A feature-rich Node.js client for Google BigQuery with support for CRUD operations, transactions, query building, and advanced features like aggregate functions, pagination, and logging.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && terser dist/index.js --compress --mangle -o dist/index.min.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "bigquery",
    "google-cloud",
    "sql",
    "database",
    "nodejs",
    "typescript",
    "query-builder",
    "bigquery-client",
    "bigquery-transaction",
    "bigquery-query",
    "cloud-database",
    "data-analytics",
    "etl",
    "data-pipeline",
    "bigquery-sdk",
    "bigquery-wrapper",
    "bigquery-orm",
    "cloud-sql",
    "gcp",
    "bigquery-api",
    "sql-query-builder",
    "database-client",
    "bq-client",
    "orm",
    "querybuilder",
    "bigquery-querybuilder",
    "bigquery-orm-tool"
  ],
  "author": "Pravin Jadhav",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/bigquery": "^7.9.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "terser": "^5.37.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pravinjadhav7/bigquery-client.git"
  },
  "bugs": {
    "url": "https://github.com/pravinjadhav7/bigquery-client/issues"
  },
  "homepage": "https://github.com/pravinjadhav7/bigquery-client#readme"
}
